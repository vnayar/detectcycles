  "configurations": [
    {
      "name": "executable",
      "targetType": "executable",
      "dflags": ["-J."],
    },
    {
      "name": "unittest",
      "excludedSourceFiles": ["src/app.d"],
      "sourcePaths": [
        "test"
      ],
      "importPaths": [
        "test"
      ],
      "targetType": "executable",
      "dflags": ["-J."],
      "preBuildCommands": ["dub run unit-threaded -c gen_ut_main -- -f bin/ut.d"],
      "mainSourceFile": "bin/ut.d",
      "dependencies": {
        "unit-threaded": "~>0.7.11"
      }
    }
  ]
}
